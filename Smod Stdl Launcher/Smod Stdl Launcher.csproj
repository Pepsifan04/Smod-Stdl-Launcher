<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>Smod_Stdl_Launcher</RootNamespace>
    <UseWPF>true</UseWPF>
	<IncludeNativeLibrariesForSelfExtract>true</IncludeNativeLibrariesForSelfExtract>
	<ApplicationIcon>Half-Life 2.ico</ApplicationIcon>
	<SignAssembly>true</SignAssembly>
	<AssemblyOriginatorKeyFile>Bsnooch.pfx</AssemblyOriginatorKeyFile>
	<DelaySign>false</DelaySign>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Images\btnBg.png" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Images\btnBg.png" />
  </ItemGroup>

</Project>
